            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;Missing parameters&quot; [14]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;Vishnu initialization failed&quot; [15]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;Undefined error&quot; [16]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;The sessionKey is expired. The session is closed.&quot; [29]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;The machine id is unknown&quot; [32]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;The batch scheduler type is unknown&quot; [101]</term>
            <listitem/>
          </varlistentry>
          <varlistentry>
            <term>&quot;The batch scheduler indicates an error&quot; [102]</term>
            <listitem/>
